<odoo>
  <data>
    <record id="res_partner_children_form_view" model="ir.ui.view">
      <field name="name">partner.children</field>
      <field name="model">res.partner</field>
      <field name="inherit_id" ref="base.view_partner_form" />
      <field name="arch" type="xml">
        <notebook position='inside'>
          <page string='Hijos'>
            <field name="children_id" widget="one2many_list">
              <tree editable= "bottom" create="false" edit="false">
                 <field name="day"/>
                 <field name="month"/>
                 <field name="year"/>
              </tree>
            </field>
          </page>
        </notebook>
      </field>
    </record>
  </data>
</odoo>